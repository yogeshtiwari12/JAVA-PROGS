import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner yt = new Scanner(System.in);

        String s = yt.nextLine();

        String []s2 = s.split(" ");
        for(int i=0;i<s2.length;i++){
            String s3 = s2[i];
            System.out.print(s3.charAt(s3.length()/2)+" ");
        }

    }
}
