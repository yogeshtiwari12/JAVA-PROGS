                                          
                                          
                                          
                                          
                                          
                                                               public void revsrse(){
                                                                 Node curr = head;
                                                                 Node prev = null;
                                                                 while (curr!=null){
                                                                     Node temp = curr.next;
                                                                     curr.next = prev;
                                                                     prev = curr;
                                                                     curr = temp;
                                                                 }
                                                            head = prev;
                                                            
                                                                }
